ul, html, body, h1, h2 {
  margin: 0;
  padding: 0;
}

ul {
  list-style none;
}

body {
  font-family: sans-serif;
}

#snippets {
  width: 1000px
  margin: 0 auto;
}

#pages {
  width: 200px
  float: left;


  a {
    color #282;
    text-decoration: none;
  }

}

#content {
  width: 800px
  margin-left: 200px

  h1 {
    margin-top: 20px
  }
  h2 {
    margin-top: 20px
    margin-bottom: 5px
  }

  ul {
    overflow hidden;
    > li {
      float: left;

      &:after {
        content: ","
        margin-left: -4px;
      }
      &:last-child:after {
        content: ""
      }

    }
  }
}